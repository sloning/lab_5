<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171"
      fx:controller="FXMLControllers.FXMLDocumentController">
    <children>
        <BorderPane VBox.vgrow="ALWAYS">
            <center>
                <GridPane prefHeight="375.0" prefWidth="541.0" BorderPane.alignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <TableView fx:id="mainTable" prefHeight="386.0" prefWidth="541.0" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
                            <columns>
                                <TableColumn fx:id="movieCol" prefWidth="75.0" text="Movie"/>
                                <TableColumn fx:id="IDCol" prefWidth="75.0" text="ID"/>
                                <TableColumn fx:id="X1Col" prefWidth="75.0" text="X"/>
                                <TableColumn fx:id="Y1Col" prefWidth="75.0" text="Y"/>
                                <TableColumn fx:id="dateCol" prefWidth="75.0" text="Date of creation"/>
                                <TableColumn fx:id="coutnCol" prefWidth="75.0" text="Oscars"/>
                                <TableColumn fx:id="lengthCol" prefWidth="75.0" text="Length"/>
                                <TableColumn fx:id="genreCol" prefWidth="75.0" text="Genre"/>
                                <TableColumn fx:id="ratingCol" prefWidth="75.0" text="Rating"/>
                                <TableColumn fx:id="direcotrCol" prefWidth="75.0" text="Режиссёр"/>
                                <TableColumn fx:id="birthDayCol" prefWidth="75.0" text="Birth day"/>
                                <TableColumn fx:id="heightCol" prefWidth="75.0" text="Height"/>
                                <TableColumn fx:id="weightCol" prefWidth="75.0" text="Weight"/>
                                <TableColumn fx:id="locCol" prefWidth="75.0" text="Location"/>
                                <TableColumn fx:id="X2Col" prefWidth="75.0" text="X"/>
                                <TableColumn fx:id="Y2Col" prefWidth="75.0" text="Y"/>
                                <TableColumn fx:id="Z2Col" prefWidth="75.0" text="Z"/>
                                <TableColumn fx:id="userCol" prefWidth="75.0" text="User"/>
                                <TableColumn fx:id="keyCol" prefWidth="75.0" text="Key"/>
                            </columns>
                        </TableView>
                    </children>
                </GridPane>
            </center>
            <left>
                <GridPane alignment="CENTER" BorderPane.alignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Label fx:id="currUserLabel" alignment="CENTER" contentDisplay="CENTER" text="User:"
                                       textAlignment="CENTER"/>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="1">
                            <children>
                                <Label fx:id="currentUser" alignment="CENTER" contentDisplay="CENTER" textAlignment="CENTER" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2">
                            <children>
                                <Button fx:id="changeUserButton" alignment="CENTER" contentDisplay="CENTER"
                                        graphicTextGap="0.0" mnemonicParsing="false" onAction="#changeUser"
                                        prefHeight="72.0" prefWidth="82.0" text="Sign in" textAlignment="CENTER"
                                        wrapText="true">
                                    <font>
                                        <Font size="10.0"/>
                                    </font>
                                </Button>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0"
                              GridPane.rowIndex="3">
                            <children>
                                <Button fx:id="helpButton" alignment="CENTER" contentDisplay="CENTER"
                                        mnemonicParsing="false" onAction="#onClickMethod" text="Help"
                                        textAlignment="CENTER"/>
                            </children>
                        </HBox>
                    </children>
                </GridPane>
            </left>
        </BorderPane>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Button fx:id="showCommand" mnemonicParsing="false" onAction="#useShowCommand" prefHeight="18.0"
                                prefWidth="66.0" text=" show"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
                    <children>
                        <Button fx:id="clearCommand" mnemonicParsing="false" prefHeight="26.0" prefWidth="66.0"
                                text="clear"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="2">
                    <children>
                        <Button fx:id="infoCommand" mnemonicParsing="false" onAction="#useInfoCommand" prefHeight="18.0"
                                prefWidth="65.0" text="info"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="3">
                    <children>
                        <Button fx:id="insertCommand" mnemonicParsing="false" onAction="#useInsertCommand"
                                prefHeight="26.0" prefWidth="66.0" text="insert"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="4">
                    <children>
                        <Button fx:id="updateCommand" mnemonicParsing="false" prefHeight="26.0" prefWidth="66.0"
                                text="update"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="5">
                    <children>
                        <Button fx:id="removeCommand" mnemonicParsing="false" prefHeight="26.0" prefWidth="66.0"
                                text="remove"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="6">
                    <children>
                        <Button fx:id="minByIDCommand" mnemonicParsing="false" prefHeight="26.0" prefWidth="66.0"
                                text="min by id"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="7">
                    <children>
                        <Button fx:id="historyCommand" mnemonicParsing="false" onAction="#useHistoryCommand"
                                prefHeight="26.0" prefWidth="66.0" text="history"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="8">
                    <children>
                        <ComboBox fx:id="langBox" onAction="#changeLanguage" prefWidth="150.0"
                                  promptText="Choose language"/>
                    </children>
                </VBox>
            </children>
        </GridPane>
    </children>
</VBox>
